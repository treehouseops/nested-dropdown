{
  "author": "Nicolas Galler <nic@f1code.com>",
  "description": "A dynamically loading nested dropdown",
  "keywords": [
    "react",
    "nested",
    "dynamic",
    "dropdown"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "name": "nested-dropdown",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicocrm/nested-dropdown"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint --ext .js .",
    "clean": "rm -rf dist",
    "build:cjs": "babel src --out-dir dist",
    "build": "npm run clean && npm run build:cjs",
    "prepublish": "npm run build",
    "test": "mocha --require babel-register --require ignore-styles test/**/*.spec.js -r ./test/setup.js --recursive",
    "start": "budo example/index.js --css example/app.css --live -- -t [ babelify ]"
  },
  "version": "1.0.0",
  "peerDependencies": {
    "react": "^15.3.2",
    "prop-types": "^15.5.8"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.1.2",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "budo": "^10.0.3",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-subset": "^1.5.0",
    "css-modulesify": "^0.28.0",
    "enzyme": "^2.4.1",
    "eslint": "^4.0.0",
    "eslint-config-f1-react": "^1.0.1",
    "faker": "^4.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.6.0",
    "mocha": "^3.0.1",
    "postcss-nested": "^1.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  }
}
